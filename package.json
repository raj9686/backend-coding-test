{
  "name": "backend-coding-test",
  "version": "1.0.0",
  "description": "A coding assessment for Xendit backend interviews",
  "main": "index.js",
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "test": "nyc mocha tests & npm run lint",
    "start": "node index.js",
    "lint": "eslint ./src --fix",
    "prepare": "husky install"
  },
  "husky": {
    "hooks": {
      "pre-push": "npm run test"
    }
  },
  "nyc": {
    "check-coverage": true,
    "branches": 80,
    "lines": 80,
    "functions": 80,
    "statements": 80
  },
  "keywords": [
    "Xendit"
  ],
  "author": "Bo",
  "license": "ISC",
  "dependencies": {
    "assert": "^2.0.0",
    "body-parser": "^1.19.0",
    "express": "^4.16.4",
    "npx": "^10.2.2",
    "sqlite3": "^4.0.6"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^2.9.0",
    "@typescript-eslint/parser": "^2.9.0",
    "eslint": "^6.7.1",
    "eslint-config-google": "^0.14.0",
    "husky": "^3.1.0",
    "mocha": "^6.1.4",
    "nyc": "^14.1.1",
    "supertest": "^4.0.2",
    "typescript": "^3.7.2",
    "winston": "^3.2.1"
  }
}
